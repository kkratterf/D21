{
  "name": "d21",
  "description": "Open-source startup directories",
  "license": "AGPL-3.0-or-later",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "ultracite lint",
