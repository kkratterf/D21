    "typescript": "^5.7.2",
    "ultracite": "^4.2.0"
  },
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "@headlessui/react": "2.2.0"
  },
  "packageManager": "pnpm@9.15.0"
}
